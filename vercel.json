{
	"rewrites": [
		{
			"source": "/acetaxi/manifest.json",
			"destination": "/manifest.json"
		},
		{
			"source": "/acetaxi/assets/(.*)",
			"destination": "/assets/$1"
		},
		{
			"source": "/acetaxi/service-worker.js",
			"destination": "/service-worker.js"
		},
		{
			"source": "/acetaxi/(.*)",
			"destination": "/index.html"
		}
	]
}
